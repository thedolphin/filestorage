[node]
hostname=img01-1
hostprefix=http://imgtest1.lan
storage=/vol/storage
hashstorage=/vol/storage.hash
hashalgo=sha256
lockdir=/dev/shm

[group]
prefix=http://imgtest.lan
index=1

[log]
commit=/vol/filestorage.log

[replica]
parents=img01-2
rabbit=rabbit@imgtest1,rabbit@imgtest2

[amqp]
user=fsuser
pass=fspass
host=127.0.0.1
port=5672
vhost=/filestorage

[db]
user=fsuser
pass=fspass
host=imgtest2.lan
db=filestorage
delete=yes
